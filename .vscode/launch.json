{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Executable/bin/Debug/net9.0/SystemOfEquations.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Executable",
            "console": "integratedTerminal",
            "stopAtEntry": false,
            "env": {
                "DOTNET_ROOT": "/usr/local/share/dotnet"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}